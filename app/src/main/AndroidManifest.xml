<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.USE_FINGERPRINT" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Android_test"
        tools:targetApi="31">
        <activity
            android:name=".hilt.ui.MainHiltActivity"
            android:exported="false" />
        <activity
            android:name=".form_with_rx.FormWithRxActivity"
            android:exported="false" />
        <activity
            android:name=".finger_print.FingerPrintActivity"
            android:exported="false" />
        <activity
            android:name=".circular_reveals.CircularRevealsActivity"
            android:exported="false" />
        <activity
            android:name=".view_page.ViewPageActivity"
            android:exported="false" />
        <activity
            android:name=".element_transition.ElementTransitionActivity"
            android:exported="false" />
        <activity
            android:name=".custom_view.CustomViewActivity"
            android:exported="false" />
        <activity
            android:name=".motion.MotionSceneActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>